{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "jest": true
  },
  "globals": {
    "$": false,
    "Dropzone": false
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "comma-dangle": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-shadow": 0,
    "no-console": 0,
    "no-plusplus": 0,
    "linebreak-style": 0,
    "no-alert": 0,
    "max-len": 0,
    "func-names": 0,
    "import/prefer-default-export": 0,
    "import/no-dynamic-require": 0
  }
}